pipeline{
  agent any
  stages{

     stage('Build'){
        steps{
       echo "Hi murali welcoem to jenkins"
       echo "Hi raghuram" 

       }

      }
 
       stage('Test'){
         steps{
         echo "Testing murali"
         echo "Testting raghuram"

        }

       }


       stage('deploy'){
             steps{
             echo "Deploy murali"
             echo "Deploy raghuram"

          }
       }


   }


}
