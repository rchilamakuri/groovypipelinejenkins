pipeline{
    environment{
        SOME_VAR = "SOME VALUE"
        CRED1 = credentials("cred1")
        INBETWEEN = "Something in between"
        CRED2 = credentials("cred2")
        OTHER_VAR = "${SOME_VAR}"
    }
    
    agent any

    stages{

        stage("foo"){
            steps{
                sh 'echo "SOME_VAR IS $SOME_VAR"'
                sh 'echo "INBETWEEN IS $INBETWEEN"'
                sh 'echo "OTHER_VAR IS $OTHER_VAR"'
            
            }
        }
    }


}
