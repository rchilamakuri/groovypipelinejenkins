import jenkins.model.Jenkins
import hudson.model.View


def first_name = "Raghuram"
def last_name = "Chilamakuri"

try{
stage('stageOne'){
       
    echo "${first_name}"
}
stage('stageTwo'){
    
    echo "${last_name}"
}
}catch{
    echo "your pipeline is working fine but you are an idiot"
}
finally{
    echo "First learn the basics and get into the field"
}


